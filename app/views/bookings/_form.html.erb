<%= simple_form_for(@booking) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.association :flat, collection: Flat.all.where(['owner_id != ?', current_user.id]) %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
